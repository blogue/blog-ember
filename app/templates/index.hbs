{{#each model.posts as |post index|}}
  <ul>
  {{#if (lt index 5)}}
    <li>{{#link-to 'post' post.id}}{{post.title}}{{/link-to}}</li>
  {{/if}}
  </ul>
{{/each}}

<ul>
  {{#each model.categories as |category|}}
  <li>{{#link-to 'category' category.id}}{{category.name}}{{/link-to}}</li>
  {{/each}}
</ul>
